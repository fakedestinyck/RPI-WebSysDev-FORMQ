$(document).ready(function(){
	$("#request1").click(function() {
		if ($("#contentx").hasClass("hidden")) {
			$("#contentx").removeClass("hidden");
			$("#expand").addClass("hidden");
		}
		else {
			$("#contentx").addClass("hidden");
			$("#expand").removeClass("hidden");
		}

	});
});

